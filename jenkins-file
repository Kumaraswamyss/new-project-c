pipeline{
    agent any
    stages{
        stage("stage1: checkout stage"){
            steps{
                git url: 'git@github.com:Kumaraswamyss/new-project-c.git'
            }
        }
        stage("stage2 : compile the code"){
            steps{
                sh "compile"
            }
        }
        stage("stage3: unit test"){
            steps{
                sh "make test"
            }
        }
        stage("stage4: QA"){
            steps{
                sh "make"
            }
        }
        stage("stage5: package"){
            steps{
                sh "/var/lib/jenkins/workspace/new-project-c"
            }
        }
    }
}
